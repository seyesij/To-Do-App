<% include ../partials/boilerplate %>

<form method='POST' action='/todos/<%= data.id %>?_method=PUT'>
  <input class='editing' name='title' type='text' value='<%= data.title %>' />
  <input class='editing' name='category' type='text' value='<%= data.category %>' />
